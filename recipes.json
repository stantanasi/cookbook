            "unit": "gousses"
          },
          {
            "name": "Purée de tomates",
            "quantity": 200,
            "unit": "g"
          },
          {
            "name": "Paprika",
            "quantity": 2,
            "unit": "cuillère à soupe"
          },
          {
            "name": "Piment de Cayenne",
            "quantity": 1,
            "unit": "cuillère à café"
          },
          {
            "name": "Mélange italien",
            "quantity": 1,
            "unit": "cuillère à soupe"
