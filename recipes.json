    "preparationTime": 10,
    "cookingTime": 20,
    "restTime": 0,
    "servings": 12,
    "steps": [
      {
        "title": "",
        "ingredients": [
          {
            "name": "Pâtes feuilletées",
            "quantity": 2,
            "unit": ""
          },
          {
            "name": "Poudre d'amande",
            "quantity": 200,
            "unit": "g"
          },
          {
            "name": "Sucre",
            "quantity": 120,
            "unit": "g"
          },
          {
            "name": "Beurre fondu",
            "quantity": 120,
            "unit": "g"
          },
          {
            "name": "Œufs",
            "quantity": 2,
            "unit": ""
          },
          {
            "name": "Jaune d'oeuf",
            "quantity": 1,
            "unit": ""
          }
        ],
        "instructions": [
          {
            "description": "Mélangez la poudre d'amande, le sucre, le beurre fondu et les œufs"
          },
          {
            "description": "Déposez une pâte feuilletée sur une plaque de cuisson recouverte de papier sulfurisé"
          },
          {
            "description": "Versez la frangipane sur toute la surface de la pâte en laissant un bord d'environ 2 ou 3 cm, que vous humidifiez avec de l'eau froide"
          },
          {
            "description": "Déposez la fève"
          },
          {
            "description": "Recouvrez avec la deuxième pâte feuilletée, en soudant bien les bords à l'aide d'une fourchette"
